/*
 * Sega Saturn USB flash cart ROM linker script
 * Anders Montonen, 2012, 2015
 *
 * Based on Bart Trzynadlowski's Custom Sega Saturn Linker Script
 * Public domain
 *
 */

SECTIONS
{
    . = 0x6004000;
    .text :
    {
        KEEP(*(.sysid))
        *(.text)
    }
    .data :
    {
        *(.data);
    }
    __bss_start = .;
    .bss : { *(.bss);  }
    __bss_end = .;
}
